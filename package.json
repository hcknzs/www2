{
	"name": "hcknzs-www2",
	"private": true,
	"sideEffects": false,
	"type": "module",
	"scriptlint": {
		"ignoreScripts": [
			"build:gql:schema"
		]
	},
	"scripts": {
		"build": "run-s build:gql:schema build:svg build:remix",
		"build:gql:schema": "source .vercel/.env.development.local && node ./node_modules/.bin/gql-tada generate-schema $CMS_ENDPOINT --header \"authorization: Bearer $CMS_API_TOKEN_READONLY\" --header \"X-Exclude-Invalid: true\" --header \"X-Include-Drafts: true\"",
		"build:remix": "remix build",
		"build:svg": "pupper-svg -i ./svg-masters -o ./public/sprite.svg  -c ./app/components/icons -m ./app/components/icons/svg-sprite.tsx -H /sprite.svg -P",
		"dev": "LOCALHOST=1 run-p dev:*",
		"dev:remix": "remix dev --manual",
		"other:typecheck": "tsc",
		"postinstall": "patch-package",
		"start": "remix-serve ./build/index.js",
		"test": "run-p test:*",
		"test:knip": "knip --exclude dependencies,unlisted",
		"test:lint": "eslint --ignore-path .gitignore --cache --cache-location ./node_modules/.cache/eslint .",
		"test:scripts": "scriptlint --strict --fix",
		"test:types": "tsc --noEmit"
	},
	"dependencies": {
		"@fontsource/ibm-plex-mono": "^5.0.12",
		"@fontsource/ibm-plex-sans": "^5.0.19",
		"@headlessui/react": "^1.7.19",
		"@peerigon/pupper": "^1.1.0",
		"@react-hookz/web": "^24.0.4",
		"@remix-run/css-bundle": "^2.8.1",
		"@remix-run/node": "^2.8.1",
		"@remix-run/react": "^2.8.1",
		"@remix-run/serve": "^2.8.1",
		"@t3-oss/env-core": "^0.9.2",
		"@tailwindcss/typography": "^0.5.12",
		"clsx": "^2.1.0",
		"dotenv": "^16.4.5",
		"gql.tada": "^1.5.3",
		"isbot": "^5.1.4",
		"react": "^18.2.0",
		"react-datocms": "^6.0.1",
		"react-dom": "^18.2.0",
		"tiny-invariant": "^1.3.3",
		"uberschrift": "^1.1.11",
		"zod": "^3.22.4"
	},
	"devDependencies": {
		"@0no-co/graphqlsp": "^1.10.1",
		"@remix-run/dev": "^2.8.1",
		"@remix-run/eslint-config": "^2.8.1",
		"@types/react": "^18.2.79",
		"@types/react-dom": "^18.2.25",
		"autoprefixer": "^10.4.19",
		"eslint": "^8.57.0",
		"eslint-config-peerigon": "^39.0.0",
		"eslint-plugin-canonical": "^4.18.0",
		"eslint-plugin-jsx-a11y": "^6.8.0",
		"eslint-plugin-react": "^7.34.1",
		"eslint-plugin-react-hooks": "^4.6.0",
		"knip": "^5.9.4",
		"npm-run-all": "^4.1.5",
		"patch-package": "^8.0.0",
		"postcss": "^8.4.38",
		"prettier": "^3.2.5",
		"prettier-plugin-tailwindcss": "^0.5.14",
		"scriptlint": "^3.0.0",
		"tailwindcss": "^3.4.3",
		"typescript": "^5.4.5"
	},
	"engines": {
		"node": ">=18.0.0"
	}
}
